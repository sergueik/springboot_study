digraph BrokenExample2 {
  rankdir=LR;
  node [shape=box];

  subgraph cluster_A {
    label="Domain A";
    style=dashed;
    A1 [label="A\\john"];
    A2 [label="Kerberos"];
  }

  subgraph cluster_B {
    label="Domain B";
    style=dashed;
    B1 [label="ForeignSecurityPrincipal"];
    B2 [label="Group"];
  }

  // Conflicting rank constraint across clusters
  { rank=same; A2; B1; }

  A1 -> A2;
  A2 -> B1;
  B1 -> B2;

  // Extra constraint that increases likelihood of warnings / crashes
  A1 -> B2 [constraint=false];
}

